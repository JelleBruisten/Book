{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "book": {
      "projectType": "application",
      "schematics": {},
      "root": "apps\\book",
      "sourceRoot": "apps\\book\\src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist\\apps\\book",
            "index": "apps/book/src/index.html",
            "main": "apps/book/src/main.ts",
            "polyfills": "apps/book/src/polyfills.ts",
            "tsConfig": "apps\\book\\tsconfig.app.json",
            "aot": true,
            "assets": ["apps/book/src/favicon.ico", "apps/book/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/book/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/book/src/environments/environment.ts",
                  "with": "apps/book/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "book:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "book:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "book:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/book/src/test.ts",
            "polyfills": "apps/book/src/polyfills.ts",
            "tsConfig": "apps\\book\\tsconfig.spec.json",
            "karmaConfig": "apps\\book\\karma.conf.js",
            "assets": ["apps/book/src/favicon.ico", "apps/book/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/book/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps\\book\\tsconfig.app.json",
              "apps\\book\\tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "book-e2e": {
      "projectType": "application",
      "root": "apps\\book-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps\\book-e2e\\protractor.conf.js",
            "devServerTarget": "book:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "book:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps\\book-e2e\\tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/interfaces/.eslintrc",
            "tsConfig": ["libs/interfaces/tsconfig.lib.json"],
            "exclude": ["**/node_modules/**", "!libs/interfaces/**"]
          }
        }
      }
    }
  },
  "defaultProject": "book",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
